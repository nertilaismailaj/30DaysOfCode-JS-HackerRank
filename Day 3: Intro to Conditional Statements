function main() {
    const N = parseInt(readLine(), 10);

    switch(N%2){
        case 1:
            console.log("Weird");
            break;
        case 0:
           if (N>2 && N<5){
                 console.log("Not Weird");
           }else if (N>6 && N<=20){
                 console.log("Weird");

           }else if (N > 20){
                 console.log("Not Weird");
           }
    }
}
